{
  "border-radius": {
    "prefix": "brr",
    "body": ["border-radius: ${1:50%};"]
  },

  "object-fit: cover;": {
    "prefix": "ofc",
    "body": ["object-fit: cover;"]
  },

  "ibg for image tag": {
    "prefix": "ibg",
    "body": ["position: absolute;", "top: 0;", "left: 0;", "width: 100%;", "height: 100%;", "object-fit: cover;"]
  },

  "flex full": {
    "prefix": "flf",
    "body": ["display: flex;", "justify-content: center;", "align-items: center;", "flex-wrap: wrap;"]
  },

  "row-gap": {
    "prefix": "rg",
    "body": ["row-gap: $1;"]
  },

  "column-gap": {
    "prefix": "cg",
    "body": ["column-gap: $1;"]
  },

  "grid container that limits max-width": {
    "prefix": "cont2",
    "body": [
      "display: grid;",
      "grid-template-columns: 1fr minmax(auto, $1px) 1fr;",
      "margin: 0 auto;",
      "padding-left: ${2:15}px;",
      "padding-rihgt: ${2:15}px;",
      "",
      "& > * {",
      "  grid-column-start: 2;",
      "}"
    ]
  },

  "absolute full width": {
    "prefix": "afs",
    "body": ["position: absolute;", "top: 0;", "left: 0;", "height: 100%;", "width: 100%;", "background-color: #;"]
  },

  "absolute center": {
    "prefix": "ac",
    "body": ["position: absolute;", "top: 50%;", "left: 50%;", "transform: translate(-50%, -50%);"]
  },

  "absolute center x": {
    "prefix": "acx",
    "body": ["position: absolute;", "left: 50%;", "transform: translateX(-50%);"]
  },

  "absolute center y": {
    "prefix": "acy",
    "body": ["position: absolute;", "top: 50%;", "transform: translateY(-50%);"]
  },

  "transform: scale();": {
    "prefix": "trs",
    "body": ["transform: scale($1);"]
  },

  "transform: ;": {
    "prefix": "tra",
    "body": ["transform: $1;"]
  },

  "transform: translate();": {
    "prefix": "trt",
    "body": ["transform: translate($1);"]
  },

  "transform: translateX();": {
    "prefix": "trtx",
    "body": ["transform: translateX($1);"]
  },

  "transform: translateY();": {
    "prefix": "trty",
    "body": ["transform: translateY($1);"]
  },

  "transform: rotate();": {
    "prefix": "trr",
    "body": ["transform: rotate($1);"]
  },

  "transform: rotateX();": {
    "prefix": "trrx",
    "body": ["transform: rotateX($1);"]
  },

  "transform: rotateY();": {
    "prefix": "trry",
    "body": ["transform: rotateY($1);"]
  },

  "transition": {
    "prefix": "tr",
    "body": ["transition: all .$1s ease;"]
  },

  "flex": {
    "prefix": "fl",
    "body": ["flex: 0 0 $1%;"]
  },

  "background url": {
    "prefix": "bgu",
    "body": ["background: url(../$1) no-repeat center center;", "background-size: co$2;"]
  },

  "hover": {
    "prefix": "hov",
    "body": ["&:hover {", "  $1", "}", ""]
  },

  "before": {
    "prefix": "bf",
    "body": [":before {", "  ", "}", ""]
  },

  "before full": {
    "prefix": "bff",
    "body": [":before {", "top: $1;", "left: $2;", "width: $3;", "height: $4;"]
  },

  "after": {
    "prefix": "af",
    "body": [":after {", "  ", "}", ""]
  },

  "after full": {
    "prefix": "aff",
    "body": [":after {", "top: $1;", "left: $2;", "width: $3;", "height: $4;"]
  },

  "border": {
    "prefix": "bor",
    "body": ["border: $1;"]
  },

  "calc()": {
    "prefix": "cc",
    "body": ["calc($1)"]
  },

  "media": {
    "prefix": "md",
    "body": ["@media(max-width: $1px) {", "  $2", "}"]
  },

  "media 767": {
    "prefix": "md7",
    "body": ["@media(max-width: 767px) {", "  $1", "}"]
  },

  "media 992": {
