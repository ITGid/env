  },

  "media 500": {
    "prefix": "md5",
    "body": ["@media(max-width: 500px) {", "  $1", "}"]
  },

  "media 425": {
    "prefix": "md4",
    "body": ["@media(max-width: 425px) {", "  $1", "}"]
  },

  "media 375": {
    "prefix": "md3",
    "body": ["@media(max-width: 375px) {", "  $1", "}"]
  },

  "media 800": {
    "prefix": "md8",
    "body": ["@media(max-width: 800px) {", "  $1", "}"]
  },

  "media 1100": {
    "prefix": "md11",
