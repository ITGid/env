  },

  "media 1440": {
    "prefix": "md14",
    "body": ["@media(max-width: 1440px) {", "  $1", "}"]
  },

  "big comment": {
    "prefix": "bigc",
    "body": [
      "",
      "/*",
      "* ---------------------------------------------------------------------------------- *",
      "* ======================================  $1  ======================================== *",
      "* ---------------------------------------------------------------------------------- *",
      "*/"
    ]
  },

  "standard comment": {
    "prefix": "mdc",
    "body": ["", "/*", "* ====================================  $1  ==================================== *", "*/"]
  },

  "small comment": {
    "prefix": "smc",
    "body": [" /* --- $1 --- */"]
  },

  "important comment": {
    "prefix": "impc",
    "body": [
      "",
      "/*",
      "! ---------------------------------------------------------------------------------- !",
      "! ======================================  $1  ======================================== !",
      "! ---------------------------------------------------------------------------------- !",
      "*/"
    ]
  },

  "burger": {
    "prefix": "burg",
    "body": [
      ".burger {",
      "  display: none;",
      "  position: relative;",
      "  width: $1;",
      "  height: $2;",
      "  z-index: 9;",
      "  cursor: pointer;",
      "}",
      "",
      ".burger:before,",
      ".burger:after {",
      "  left: 50%;",
      "  width: 100%;",
      "  height: $3;",
      "  background-color: $4;",
      "  transition: 0.5s;",
      "  transform: translateX(-50%);",
      "}",
      "",
      ".burger:before {",
      "  top: 0;",
      "}",
      "",
      ".burger:after {",
      "  bottom: 0;",
      "}",
      "",
      ".burger span {",
      "  position: absolute;",
      "  top: 50%;",
      "  left: 50%;",
      "  width: 100%;",
      "  height: $5;",
      "  background-color: $6;",
      "  transition: 0.5s;",
      "  transform: translate(-50%, -50%);",
      "}",
      "",
      ".burger.active span:first-child {",
      "  transform: translate(-50%, -50%) rotate(45deg);",
      "}",
      "",
      ".burger.active span:last-child {",
      "  transform: translate(-50%, -50%) rotate(-45deg);",
      "}",
      "",
      ".burger.active:before,",
      ".burger.active:after {",
      "  transform: translateX(-50%);",
      "  width: 0;",
      "  height: 0;",
      "  opacity: 0;",
      "}",
      ""
    ]
  },

  "start snippet": {
    "prefix": "start",
    "body": [
      "",
      "/*",
      "* -------------------------------------------------------------------------------------------- *",
      "* ====================================  CONNECTING FONTS  ==================================== *",
      "* -------------------------------------------------------------------------------------------- *",
