      "}",
      "",
      "",
      "",
      "/*",
      "* -------------------------------------------------------------------------------------------- *",
      "* ====================================  GENERAL CLASSES  ===================================== *",
      "* -------------------------------------------------------------------------------------------- *",
      "*/",
      "",
      ".wrapper {",
      "  overflow: hidden;",
      "  display: flex;",
      "  flex-direction: column;",
      "  justify-content: stretch;",
      "  min-height: 100vh;",
      "}",
      "",
      ".content {",
      "  flex: 1 1 auto;",
      "}",
      "",
